testScorefield = input("Enter the student's test score: ")  #Student Test Score
classRankfield = input("Enter the student's class rank: ")  #Student Class Rank

testScore = int(testScorefield)
classRank = int(classRankfield)


if testScore >= 90:
    if classRank >= 25:
        print("Accept")
    else:
        print("Reject")
else:  
    if testScore >= 80:
        if classRank >= 50:
            print("Accept")
        else:
            print("Reject")
    else:  
        if testScore >= 70:
            if classRank >= 75:
                print("Accept")
            else:
                print("Reject")
        else: 
            print("Reject")
